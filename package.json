  "scripts": {
    "//": [
      "the build step will pipe all our js through babel as well because webpack is using ",
      "babel-loader. See webpack.config.js.",
      "",
      "NODE_PATH=. is necessary on our jest steps so that we can require modules using ",
      "absolute paths (we've set the same thing up in Node, see the top of app.js)",
      "--env jest-environment-node-debug is necessary for devtools to pick up 'debugger;' statements",
      "in our tests (see https://github.com/facebook/jest/issues/1652#issuecomment-265423666)."
    ],
    "build": "yarn run webpack",
    "start": "node app.js",
    "go": "yarn build && yarn start",
    "test": "NODE_PATH=. yarn jest",
    "testd": "NODE_PATH=. node --inspect --debug-brk node_modules/.bin/jest --runInBand --env jest-environment-node-debug"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/test/jest_typescript_preprocessor.js"
    },
    "testMatch": [
      "**/test/**/*.test.(ts|tsx)"
    ]
  }
